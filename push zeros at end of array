pushZerosToEnd(arr,n){
        let c=0,zerocount=0;
        for(let i=0;i<arr.length;i++){
            if(arr[i]!=0){
                arr[c]=arr[i];
                c++;
            }
            else{
                zerocount++;
            }
        }
        for(let i=arr.length-zerocount;i<arr.length;i++){
            arr[i]=0;
        }
        return arr;
    }
